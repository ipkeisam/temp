{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "cloudformation:*",
                "s3:ListAllMyBuckets",
                "s3:GetBucketLocation",
                "sns:ListTopics"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::903921672561-cfn-templates",
                "arn:aws:s3:::cg-lab-cfn-templates"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::903921672561-cfn-templates/*",
                "arn:aws:s3:::cg-lab-cfn-templates/*"
            ]
        }
    ]
}